    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "./experiments/minio/consumer",
            "args": ["-n", "1", "-l"],
        },
        {
            "name": "Launch Remote",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "/home/ec2-user/consumer",
            "port": 2345,
            "host": "localhost",
            "cwd": "${workspaceFolder}",
            "trace": "verbose"
        }
    ]
}